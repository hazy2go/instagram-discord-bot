# Discord Bot Configuration
DISCORD_TOKEN=your_discord_bot_token_here
DISCORD_CLIENT_ID=your_discord_client_id_here

# Instagram Monitoring Configuration
# Check interval in minutes (minimum 5 recommended to avoid rate limits)
CHECK_INTERVAL=5

# Admin Role ID (users with this role can use admin commands)
ADMIN_ROLE_ID=your_admin_role_id_here

# Optional: RSS Bridge Instance URL (if self-hosting)
# RSS_BRIDGE_URL=https://your-rssbridge-instance.com

# Optional: Backup check using direct scraping (requires additional setup)
# ENABLE_BACKUP_SCRAPER=false
# PROXY_URL=http://your-proxy:port
